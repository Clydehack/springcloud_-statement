<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="context1">

		<!--取消注释 插件会生成很多注释 -->
		<commentGenerator>
			<property name="suppressDate" value="true" />
			<property name="suppressAllComments" value="true" />
		</commentGenerator>

		<!-- 连接数据库 connectionURL： -->
		<jdbcConnection
			connectionURL="jdbc:mysql://47.105.162.113:3306/uzone"
			driverClass="com.mysql.jdbc.Driver" password="" userId="root" />

		<!-- 生成pojo位置 -->
		<javaModelGenerator
			targetPackage="com.uzone.settlement.model"
			targetProject="springcloud_statement" >
			<property name="enableSubPackages" value="true" />
            <!-- 从数据库返回的值被清理前后的空格  -->
            <property name="trimStrings" value="true" />
        </javaModelGenerator>
        
		<!-- mapper生成位置 -->
		<sqlMapGenerator
			targetPackage="com.uzone.settlement.model"
			targetProject="springcloud_statement" />
			
		<!-- mapper接口生成的位置 -->
		<javaClientGenerator
			targetPackage="com.uzone.settlement.model"
			targetProject="springcloud_statement"
			type="XMLMAPPER" >
			<property name="enableSubPackages" value="true" />
        </javaClientGenerator>

		<!-- 生成那些表，tableName对应数据库表，domainObjectName对应实体类名 -->   
        <table tableName="bu_checkaccount_log" domainObjectName="BuCheckaccountLogPO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="bu_checkaccount_exp" domainObjectName="BuCheckaccountExpPO" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"></table>
		 
		<!-- 有些表的字段需要指定java类型 
		<table schema="" tableName=""> 
			<columnOverride column="" javaType="" /> 
		</table>
		 -->

	</context>
</generatorConfiguration>